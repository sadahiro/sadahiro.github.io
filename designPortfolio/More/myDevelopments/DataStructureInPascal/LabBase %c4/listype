unit listype;
{Programmer:	Makoto Sadahiro }
{starting date: Nov 9th '93}
{last modified date:  Nov 28nd '93}

{type for unit queueADT}

interface
	uses
		time;
	const
		nameLength = 20;				{change the length of name here}
		defHr = 300;				{defined amount of hours}
	type
{KeyType}
		KeyType = integer;				{difining the type of key for list here}
{ListElementType}
		ListElementType = record
				key: keytype;		{student ID}
				name: string[nameLength];
				validity: boolean;
				maxHr, usdHr, remHr: integer;
				penalty: boolean;
				atstatus: boolean;
				SinTime: timetype;
				SoutTime: timetype;
			end;				{declaration}
{*************************************}

implementation

{*************************************}

end.			{listype}